<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.baomidou.mapper.AdminAttendanceAddressMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.baomidou.entity.AdminAttendanceAddress">
        <id column="address_id" property="AddressId" />
        <result column="address_name" property="AddressName" />
        <result column="address_info" property="AddressInfo" />
        <result column="scope" property="Scope" />
        <result column="lng" property="Lng" />
        <result column="lat" property="Lat" />
        <result column="user_id" property="UserId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        address_id, address_name, address_info, scope, lng, lat, user_id
    </sql>

</mapper>
